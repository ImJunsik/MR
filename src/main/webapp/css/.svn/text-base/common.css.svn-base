@charset "utf-8";

@font-face {
    font-family:'NanumGothic';
    font-weight: normal;
    font-style: normal;
    src: url('fonts/NanumGothic.eot');
    src: url('fonts/NanumGothic.eot?#iefix') format('embedded-opentype'),
         url('fonts/NanumGothic.woff') format('woff'),
         url('fonts/NanumGothic.ttf') format('truetype');
}

@font-face {
    font-family:'NanumGothic';
    font-weight:600;
    font-style: normal;
    src: url('fonts/NanumGothicBold.eot');
    src: url('fonts/NanumGothicBold.eot?#iefix') format('embedded-opentype'),
         url('fonts/NanumGothicBold.woff') format('woff'),
         url('fonts/NanumGothicBold.ttf') format('truetype');
}

/* Correct `block` display not defined in IE 6/7/8/9 and Firefox 3 */
article,aside,details,figcaption,figure,footer,header,hgroup,main,nav,section,summary{display:block;}

/* Common */
html,body{width:100%;height:100%;}
body,p,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,table,th,td,form,fieldset,legend,input,textarea,button,select{margin:0;padding:0;*word-break:break-all;-ms-word-break:break-all;font-family:'NanumGothic','나눔고딕','Apple SD Gothic Neo',Helvetica,sans-serif;}
body,button,input,select,textarea,table{font-size:12px;color:#555;letter-spacing:0px;word-spacing:1px;}
body{background-color:#fff;text-align:center;}
img,fieldset{border:0;vertical-align:middle}
li,ul,ol{list-style:none}
input,select,button{vertical-align:middle;border-radius:2px;}
input,textarea{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;ime-mode:active;}
checkbox{width:13px;height:13px;margin:0;padding:0;vertical-align:middle;}
select{height:21px;text-indent:3px; *text-indent:0; *padding-left:3px;margin:0;font-size:12px;line-height:1.42857143;color:#555;border:1px solid #e5e5e5;}
i,em,address{font-style:normal}
legend{*margin-left:-7px}
table{border-collapse:collapse;border-spacing:0;}
b,strong{font-weight:600;}
a{table-decoration:none; color:#fff}
a:hover,a:active,a:focus{table-decoration:none;}
button *{position:relative}
button img{left:-3px;*left:auto}
.blind{visibility:hidden;overflow:hidden;position:absolute;top:0;left:0;width:0px;height:0px;font-size:0;line-height:0}
.tl{text-align:left !important}
.tc{text-align:center !important}
.tr{text-align:right !important}
pre{margin:0;padding:0;word-break:break-all;white-space:pre-line;color:#555;letter-spacing:0px;word-spacing:1px;font-family:'NanumGothic','나눔고딕','Apple SD Gothic Neo',Helvetica,sans-serif;}
hr{height:0;margin:0;padding:0;border:0;}
.divider_none{height:20px;margin:0;padding:0;border:0;}
.divider_none_5{height:5px;margin:0;padding:0;border:0;}
.divider_none_10{height:10px;margin:0;padding:0;border:0;}
.divider_solid{margin:20px 0;border:none;border-top:1px solid #e5e5e5;}
.divider_double{margin:20px 0;border:none;border-top:3px double #e5e5e5;}
.divider_dashed{margin:0;border:none;border-top:1px dashed #e5e5e5;}
.cursor{cursor:pointer;}
.vat{vertical-align:top !important;}
.vam{vertical-align:middle !important;}
.vab{vertical-align:bottom !important;}
.center{text-align:center !important;}
.box{display:inline-block !important;}

/* text */
h1,h2,h3,h4,h5,h6,.h1,.h2,.h3,.h4,.h5,.h6{font-weight:600;line-height:1.1;color:inherit;}
h1 small,h2 small,h3 small,h4 small,h5 small,h6 small,.h1 small,.h2 small,.h3 small,.h4 small,.h5 small,.h6 small,h1 .small,h2 .small,h3 .small,h4 .small,h5 .small,h6 .small,.h1 .small,.h2 .small,.h3 .small,.h4 .small,.h5 .small,.h6 .small{font-weight:normal;line-height:1;color:#333;}
h1 small,.h1 small,h2 small,.h2 small,h3 small,.h3 small,h1 .small,.h1 .small,h2 .small,.h2 .small,h3 .small,.h3 .small{font-size:65%;}
h4 small,.h4 small,h5 small,.h5 small,h6 small,.h6 small,h4 .small,.h4 .small,h5 .small,.h5 .small,h6 .small,.h6 .small{font-size:75%;}
h1,.h1{font-size: 36px;}
h2,.h2{font-size: 30px;}
h3,.h3{font-size: 24px;}
h4,.h4{font-size: 18px;}
h5,.h5{font-size: 14px;}
h6,.h6{font-size: 12px;}
.red{color:#d9534f !important;}
.red2{color:#ef5249 !important;}
.num{text-align:right !important;}
.bold{font-weight:600 !important;}

/* padding */
.pl3{padding-left:3px;}
.pl5{padding-left:5px;}
.pt3{padding-top:3px;}

