<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mr.main">

	<!-- resultMap id="summaryMap" type="com.mr.main.domain.MrSummary">
        <result property="procStCd" column="PROC_ST_CD" />
        <result property="col1" column="COL1" />
        <result property="col2" column="COL2" />
        <result property="col3" column="COL3" />
        <result property="col4" column="COL4" />
        <result property="col5" column="COL5" />
        <result property="col6" column="COL6" />
        <result property="col7" column="COL7" />
        <result property="col8" column="COL8" />
        <result property="col9" column="COL9" />

    </resultMap-->
    <!-- yoo 241104 Mybatis XML 수정 기존 메소드는 한자리수  Z01x ~ Z09x까지만 지원 하므로 새로 생성 함 Z025추가 -->
    <resultMap id="summaryMapEx" type="com.mr.main.domain.MrSummaryEx">
        <result property="procStCd" column="PROC_ST_CD" />
        <result property="col01" column="COL01" />
        <result property="col02" column="COL02" />
        <result property="col03" column="COL03" />
        <result property="col04" column="COL04" />
        <result property="col05" column="COL05" />
        <result property="col06" column="COL06" />
        <result property="col07" column="COL07" />
        <result property="col08" column="COL08" />
        <result property="col09" column="COL09" />
		<result property="col10" column="COL10" />
    </resultMap>
	
    <select id="selectSummary" resultMap="summaryMapEx">
<!-- SUM(CASE WHEN DET.MR_STEP_CD IN ('Z00R0','Z00P0') THEN 1 ELSE 0 END) AS COL6,  -->
		--selectSummary
                SELECT PROC_ST_CD,
                       SUM(COL01) AS COL01,
                       SUM(COL02) AS COL02,
                       SUM(COL03) AS COL03,
                       SUM(COL04) AS COL04,
                       SUM(COL05) AS COL05,
                       SUM(COL06) AS COL06,
                       SUM(COL07) AS COL07,
                       SUM(COL08) AS COL08,
                       SUM(COL09) AS COL09,
                       SUM(COL10) AS COL10
                  FROM (SELECT CASE WHEN DET.PROC_ST_CD IS NULL THEN 'Z0121'
                                    WHEN STEP.PROC_ST_CD IS NULL THEN  DET.PROC_ST_CD
                                    WHEN STEP.MR_STEP_CD = 'Z0030' AND STEP.PROC_ST_CD = 'Z0133' THEN 'Z0110'
                                    ELSE STEP.PROC_ST_CD END AS PROC_ST_CD,
                               SUM(CASE WHEN STEP.MR_STEP_CD = 'Z0010' THEN 1 ELSE 0 END) AS COL01,
                               SUM(CASE WHEN STEP.MR_STEP_CD = 'Z0020' THEN 1 ELSE 0 END) AS COL02,
                               SUM(CASE WHEN STEP.MR_STEP_CD = 'Z0025' THEN 1 ELSE 0 END) AS COL03, 
                               SUM(CASE WHEN STEP.MR_STEP_CD = 'Z0030' AND STEP.PROC_ST_CD IN ('Z0110','Z0121','Z0141') THEN 1 ELSE 0 END) AS COL04,
                               SUM(CASE WHEN STEP.MR_STEP_CD = 'Z0030' AND DET.PROC_ST_CD IN ('Z0133','Z0122','Z0131') THEN 1 ELSE 0 END) AS COL05,
                               SUM(CASE WHEN STEP.MR_STEP_CD = 'Z0040' THEN 1 ELSE 0 END) AS COL06,
                               SUM(DECODE(GRP_HIST.MR_RV_RST_NO,0,(CASE WHEN DET.MR_STEP_CD IN ('Z00R0','Z00P0') THEN 1 ELSE 0 END),0)) AS COL07,
                               <!-- SUM(CASE WHEN DET.MR_STEP_CD = 'Z00P0' THEN 1 ELSE 0 END) AS COL7, -->
                               SUM(CASE WHEN STEP.MR_STEP_CD = 'Z0050' THEN 1 ELSE 0 END) AS COL08,
                               SUM(CASE WHEN STEP.MR_STEP_CD IN ('Z0060','Z0070') THEN 1 ELSE 0 END) AS COL09,
                               SUM(CASE WHEN STEP.MR_STEP_CD IN ('Z0080','Z0090') THEN 1 ELSE 0 END) AS COL10
                          FROM MR_REQ_MST MR,
						       MR_REQ_PROC_STEP STEP,
                               MR_REQ_PROC_STEP_DET DET,
                              (SELECT CHRG_EMP_NO,
                                      MIN(HIST.MR_REQ_PROC_STEP_DET_NO) AS MR_REQ_PROC_STEP_DET_NO,
                                      GRP_STEP.MR_REQ_NO,
                                      GRP_STEP.MR_STEP_CD,
                                      1 MR_RV_RST_NO
                                FROM  MR_REQ_CHRGR_CHG_HIST HIST,
                                     (SELECT STEP.MR_REQ_NO,
                                             STEP.MR_STEP_CD,
                                             MIN(DET.MR_REQ_PROC_STEP_DET_NO) AS MR_REQ_PROC_STEP_DET_NO
                                        FROM MR_REQ_MST MR,
						                     MR_REQ_PROC_STEP STEP,
                                             MR_REQ_PROC_STEP_DET DET,
                                             MR_REQ_CHRGR_CHG_HIST HIST
                                       WHERE MR.MR_REQ_NO = DET.MR_REQ_NO
						                 AND STEP.MR_REQ_PROC_STEP_NO = DET.MR_REQ_PROC_STEP_NO
                                         AND STEP.MR_STEP_CD = DET.MR_STEP_CD
                                         AND DET.MR_REQ_PROC_STEP_DET_NO = HIST.MR_REQ_PROC_STEP_DET_NO
                                         --AND DET.FST_PROC_TRM_DT IS NOT NULL       /*2025-09-09 ijs 허세욱책임 요청*/ 
                                         AND HIST.CHRGR_CL_CD NOT LIKE 'Z02H%'
                                         AND STEP.PROC_ST_CD != 'Z0143'
                                         AND STEP.EFF_END_DTM = '99991231235959'
                                         AND HIST.EFF_END_DTM = '99991231235959'
                                         AND HIST.CHRG_EMP_NO = #{loginEmpNo}
                                         AND DET.MR_REQ_PROC_STEP_DET_NO NOT IN  (SELECT DET.MR_REQ_PROC_STEP_DET_NO 
						                                                           FROM MR_REQ_PROC_STEP_DET DET
						                                                           JOIN MR_REQ_CHRGR_CHG_HIST HIST
						                                                             ON DET.MR_REQ_PROC_STEP_DET_NO =HIST.MR_REQ_PROC_STEP_DET_NO
						                                                            AND HIST.DEL_YN = 0 
						                                                            AND HIST.EFF_END_DTM = '99991231235959'
						                                                            AND HIST.CHRGR_CL_CD IN ('Z02C','Z02F')
						                                                          WHERE DET.MR_STEP_CD = 'Z0010'
						                                                            AND DET.DEL_YN = 0) 
   		                                 AND MR.DEL_YN=0
                                         AND DET.DEL_YN=0
                                         AND HIST.DEL_YN=0
                                         GROUP BY STEP.MR_REQ_NO, STEP.MR_STEP_CD
                                      UNION ALL
                                      SELECT STEP.MR_REQ_NO,
                                             STEP.MR_STEP_CD,
                                             MIN(DET.MR_REQ_PROC_STEP_DET_NO) AS MR_REQ_PROC_STEP_DET_NO
                                        FROM MR_REQ_MST MR,
						                     MR_REQ_PROC_STEP STEP,
                                             MR_REQ_PROC_STEP_DET DET,
                                             MR_REQ_CHRGR_CHG_HIST HIST
                                       WHERE MR.MR_REQ_NO = DET.MR_REQ_NO
						                 AND STEP.MR_REQ_PROC_STEP_NO = DET.MR_REQ_PROC_STEP_NO
                                         AND STEP.MR_STEP_CD = DET.MR_STEP_CD
                                         AND DET.MR_REQ_PROC_STEP_DET_NO = HIST.MR_REQ_PROC_STEP_DET_NO
                                         AND DET.FST_PROC_TRM_DT IS NOT NULL
                                         AND STEP.MR_STEP_CD IN ('Z0020')
                                         AND STEP.PROC_ST_CD IN ('Z0121')
                                         AND STEP.PROC_ST_CD != 'Z0143'
                                         AND HIST.CHRGR_CL_CD NOT LIKE 'Z02H%'
                                         AND STEP.EFF_END_DTM = '99991231235959'
                                         AND HIST.EFF_END_DTM = '99991231235959'
                                         AND HIST.CHRG_EMP_NO = #{loginEmpNo}
                                         AND HIST.CHRGR_CL_CD IN ('Z02F')
				                         AND MR.DEL_YN=0
                                         AND DET.DEL_YN=0
                                         AND HIST.DEL_YN=0
                                         GROUP BY STEP.MR_REQ_NO, STEP.MR_STEP_CD
                                      UNION ALL
                                      SELECT STEP.MR_REQ_NO,
                                             STEP.MR_STEP_CD,
                                             MIN(DET.MR_REQ_PROC_STEP_DET_NO) AS MR_REQ_PROC_STEP_DET_NO
                                        FROM MR_REQ_MST MR,
						                     MR_REQ_PROC_STEP STEP,
                                             MR_REQ_PROC_STEP_DET DET,
                                             MR_REQ_CHRGR_CHG_HIST HIST
                                       WHERE MR.MR_REQ_NO = DET.MR_REQ_NO
						                 AND STEP.MR_REQ_PROC_STEP_NO = DET.MR_REQ_PROC_STEP_NO
                                         AND STEP.MR_STEP_CD = DET.MR_STEP_CD
                                         AND DET.MR_REQ_PROC_STEP_DET_NO = HIST.MR_REQ_PROC_STEP_DET_NO
                                         AND DET.FST_PROC_TRM_DT IS NOT NULL
                                         AND STEP.MR_STEP_CD IN ('Z0010')
                                         AND STEP.PROC_ST_CD NOT IN ('Z0110','Z0141','Z0143')
                                         AND HIST.CHRGR_CL_CD NOT LIKE 'Z02H%'
                                         AND STEP.EFF_END_DTM = '99991231235959'
                                         AND HIST.EFF_END_DTM = '99991231235959'
                                         AND HIST.CHRGR_CL_CD IN ('Z02C')
				                         AND MR.DEL_YN=0
                                         AND DET.DEL_YN=0
                                         AND HIST.DEL_YN=0
                                         GROUP BY STEP.MR_REQ_NO, STEP.MR_STEP_CD                                      
									  UNION ALL
                                      SELECT STEP.MR_REQ_NO,
                                             STEP.MR_STEP_CD,
                                             DET.MR_REQ_PROC_STEP_DET_NO
                                        FROM MR_REQ_MST MR,
						                     MR_REQ_PROC_STEP STEP,
                                             MR_REQ_PROC_STEP_DET DET,
                                             MR_REQ_CHRGR_CHG_HIST HIST
                                       WHERE MR.MR_REQ_NO = DET.MR_REQ_NO
						                 AND STEP.MR_REQ_PROC_STEP_NO = DET.MR_REQ_PROC_STEP_NO
                                         AND STEP.MR_STEP_CD = DET.MR_STEP_CD
                                         AND DET.MR_REQ_PROC_STEP_DET_NO = HIST.MR_REQ_PROC_STEP_DET_NO
                                         AND DET.FST_PROC_TRM_DT IS NOT NULL
                                         AND STEP.PROC_ST_CD != 'Z0143'
                                         AND HIST.CHRGR_CL_CD LIKE 'Z02H%'
                                         AND STEP.EFF_END_DTM = '99991231235959'
                                         AND HIST.EFF_END_DTM = '99991231235959'
                                         AND HIST.CHRG_EMP_NO = #{loginEmpNo}
				                         AND MR.DEL_YN=0
                                         AND DET.DEL_YN=0
                                         AND HIST.DEL_YN=0
                                      UNION ALL
                                      SELECT STEP.MR_REQ_NO,
                                             STEP.MR_STEP_CD,
                                             DET.MR_REQ_PROC_STEP_DET_NO
                                        FROM MR_REQ_MST MR,
						                     MR_REQ_PROC_STEP STEP,
                                             MR_REQ_PROC_STEP_DET DET,
                                             MR_REQ_CHRGR_CHG_HIST HIST
                                       WHERE MR.MR_REQ_NO = DET.MR_REQ_NO
						                     AND STEP.MR_REQ_PROC_STEP_NO = DET.MR_REQ_PROC_STEP_NO
                                             AND DET.MR_STEP_CD = 'Z00I0'
                                             AND DET.MR_REQ_PROC_STEP_DET_NO = HIST.MR_REQ_PROC_STEP_DET_NO
                                             AND DET.FST_PROC_TRM_DT IS NOT NULL
                                             AND STEP.PROC_ST_CD != 'Z0143'
                                             AND HIST.CHRGR_CL_CD = 'Z02I'
                                             AND STEP.EFF_END_DTM = '99991231235959'
                                             AND HIST.EFF_END_DTM = '99991231235959'
                                             AND HIST.CHRG_EMP_NO = #{loginEmpNo}
				                             AND MR.DEL_YN=0
                                             AND DET.DEL_YN=0
                                             AND HIST.DEL_YN=0
                                       UNION ALL
                                       SELECT STEP.MR_REQ_NO,
                                             DET.MR_STEP_CD,
                                             DET.MR_REQ_PROC_STEP_DET_NO
                                        FROM MR_REQ_MST MR,
						                     MR_REQ_PROC_STEP STEP,
                                             MR_REQ_PROC_STEP_DET DET,
                                             MR_REQ_CHRGR_CHG_HIST HIST
                                       WHERE MR.MR_REQ_NO = DET.MR_REQ_NO
						                     AND STEP.MR_REQ_PROC_STEP_NO = DET.MR_REQ_PROC_STEP_NO
                                             AND STEP.MR_STEP_CD = 'Z0040'
                                             AND STEP.PROC_ST_CD = 'Z0110'
                                             AND DET.MR_STEP_CD = 'Z00I0'
                                             AND DET.PROC_ST_CD IS NULL
                                             AND DET.FST_PROC_TRM_DT IS NOT NULL
                                             AND DET.MR_REQ_PROC_STEP_DET_NO = HIST.MR_REQ_PROC_STEP_DET_NO
                                             AND STEP.PROC_ST_CD != 'Z0143'
                                             AND HIST.CHRGR_CL_CD = 'Z02C'
                                             AND STEP.EFF_END_DTM = '99991231235959'
                                             AND HIST.EFF_END_DTM = '99991231235959'
                                             AND HIST.CHRG_EMP_NO =  #{loginEmpNo}
				                             AND MR.DEL_YN=0
                                             AND DET.DEL_YN=0
                                             AND HIST.DEL_YN=0
                                         )GRP_STEP
                               WHERE HIST.MR_REQ_PROC_STEP_DET_NO = GRP_STEP.MR_REQ_PROC_STEP_DET_NO
                                 AND HIST.CHRG_EMP_NO = #{loginEmpNo}
                              GROUP BY GRP_STEP.MR_REQ_NO, HIST.CHRG_EMP_NO, GRP_STEP.MR_STEP_CD
                              UNION ALL
                              SELECT HIST.CHRG_EMP_NO,
                                     MIN(DET.MR_REQ_PROC_STEP_DET_NO) MR_REQ_PROC_STEP_DET_NO,
                                     DET.MR_REQ_NO,
                                     DET.MR_STEP_CD,
                                     NVL(RST.MR_RV_RST_NO,0) MR_RV_RST_NO
                                FROM MR_REQ_MST MR,
						             MR_REQ_PROC_STEP STEP,
                                     MR_REQ_PROC_STEP_DET DET,
                                     MR_REQ_CHRGR_CHG_HIST HIST,
                                     MR_RV_RST RST
                               WHERE MR.MR_REQ_NO  = DET.MR_REQ_NO
						         AND DET.MR_REQ_NO = STEP.MR_REQ_NO
                                 AND DET.MR_REQ_PROC_STEP_DET_NO = HIST.MR_REQ_PROC_STEP_DET_NO
                                 AND DET.FST_PROC_TRM_DT IS NOT NULL
                                 AND DET.MR_STEP_CD IN ('Z00P0', 'Z00R0')
                                 AND STEP.PROC_ST_CD != 'Z0143'
                                 AND STEP.MR_STEP_CD IN ('Z0020', 'Z0025','Z0030', 'Z0040')
                                 AND STEP.EFF_END_DTM = '99991231235959'
                                 AND HIST.EFF_END_DTM = '99991231235959'
                                 AND HIST.CHRG_EMP_NO = #{loginEmpNo}
				                 AND MR.DEL_YN=0
                                 AND DET.DEL_YN=0
                                 AND STEP.DEL_YN = 0
                                 AND HIST.DEL_YN=0
                                 AND RST.MR_REQ_NO(+) = DET.MR_REQ_NO
                                 AND RST.MR_REQ_PROC_STEP_DET_NO(+) = DET.MR_REQ_PROC_STEP_DET_NO
                                 AND RST.DEL_YN(+)    = 0
                               GROUP BY DET.MR_REQ_NO, HIST.CHRG_EMP_NO, DET.MR_STEP_CD, MR_RV_RST_NO
                               )GRP_HIST
                         WHERE MR.MR_REQ_NO = DET.MR_REQ_NO
						   AND STEP.MR_REQ_PROC_STEP_NO(+) = DET.MR_REQ_PROC_STEP_NO
                           AND STEP.MR_STEP_CD(+) = DECODE(DET.MR_STEP_CD,'Z00I0','Z0040',DET.MR_STEP_CD)
                           AND GRP_HIST.MR_REQ_PROC_STEP_DET_NO = DET.MR_REQ_PROC_STEP_DET_NO
						   AND MR.DEL_YN=0
                           AND GRP_HIST.CHRG_EMP_NO = #{loginEmpNo}
                         GROUP BY STEP.MR_STEP_CD,STEP.PROC_ST_CD, DET.PROC_ST_CD
                         ORDER BY STEP.PROC_ST_CD)
                GROUP BY PROC_ST_CD
    </select>

    <select id="selectAppSummary" resultMap="summaryMapEx">
<!-- NVL(SUM(CASE WHEN DET.MR_STEP_CD IN ('Z00R0','Z00P0') THEN 1 ELSE 0 END),0) AS COL6,  -->
		--selectAppSummary
                SELECT NVL(SUM(CASE WHEN STEP.MR_STEP_CD = 'Z0010' THEN 1 ELSE 0 END),0) AS COL01,
                       SUM(CASE WHEN STEP.MR_STEP_CD = 'Z0020' THEN 1 ELSE 0 END) AS COL02,
                       NVL(SUM(CASE WHEN STEP.MR_STEP_CD = 'Z0025' THEN 1 ELSE 0 END),0) AS COL03,
                       SUM(CASE WHEN STEP.MR_STEP_CD = 'Z0030' AND STEP.PROC_ST_CD IN ('Z0110','Z0121','Z0141') THEN 1 ELSE 0 END) AS COL04,
                       SUM(CASE WHEN STEP.MR_STEP_CD = 'Z0030' AND DET.PROC_ST_CD IN ('Z0133','Z0122','Z0131') THEN 1 ELSE 0 END) AS COL05,
                       NVL(SUM(CASE WHEN STEP.MR_STEP_CD = 'Z0040' THEN 1 ELSE 0 END),0) AS COL06,
                       NVL(SUM(DECODE(GRP_HIST.MR_RV_RST_NO,0,(CASE WHEN DET.MR_STEP_CD IN ('Z00R0','Z00P0') THEN 1 ELSE 0 END),0)),0) AS COL07,
                       NVL(SUM(CASE WHEN STEP.MR_STEP_CD = 'Z0050' THEN 1 ELSE 0 END),0) AS COL08,
                       NVL(SUM(CASE WHEN STEP.MR_STEP_CD IN ('Z0060','Z0070') THEN 1 ELSE 0 END),0) AS COL09,
                       NVL(SUM(CASE WHEN STEP.MR_STEP_CD IN ('Z0080','Z0090') THEN 1 ELSE 0 END),0) AS COL10
                  FROM MR_REQ_MST MR,
				       MR_REQ_PROC_STEP STEP,
                       MR_REQ_PROC_STEP_DET DET,
                      (SELECT CHRG_EMP_NO,
                              MIN(HIST.MR_REQ_PROC_STEP_DET_NO) AS MR_REQ_PROC_STEP_DET_NO,
                              GRP_STEP.MR_REQ_NO,
                              GRP_STEP.MR_STEP_CD,
                              1 MR_RV_RST_NO
                        FROM MR_REQ_CHRGR_CHG_HIST HIST,
                             (SELECT STEP.MR_REQ_NO,
                                     MIN(DET.MR_REQ_PROC_STEP_DET_NO) AS MR_REQ_PROC_STEP_DET_NO,
                                     STEP.MR_STEP_CD
                                FROM MR_REQ_MST MR,
				                     MR_REQ_PROC_STEP STEP,
                                     MR_REQ_PROC_STEP_DET DET,
                                     MR_REQ_CHRGR_CHG_HIST HIST
                               WHERE MR.MR_REQ_NO = DET.MR_REQ_NO
      		                     AND STEP.MR_REQ_PROC_STEP_NO = DET.MR_REQ_PROC_STEP_NO
                                 AND STEP.MR_STEP_CD = DET.MR_STEP_CD
                                 AND DET.MR_REQ_PROC_STEP_DET_NO = HIST.MR_REQ_PROC_STEP_DET_NO
                                 --AND DET.FST_PROC_TRM_DT IS NOT NULL     /*2025-09-09 ijs 허세욱책임 요청*/
                                 AND HIST.CHRGR_CL_CD NOT LIKE 'Z02H%'
                                 AND STEP.PROC_ST_CD != 'Z0143'
                                 AND STEP.EFF_END_DTM = '99991231235959'
                                 AND HIST.EFF_END_DTM = '99991231235959'
                                 AND HIST.CHRG_EMP_NO = #{loginEmpNo}
				                 AND MR.DEL_YN=0
                                 AND DET.DEL_YN=0
                                 AND HIST.DEL_YN=0
                                 GROUP BY STEP.MR_REQ_NO, STEP.MR_STEP_CD
                              UNION ALL
                              SELECT STEP.MR_REQ_NO,
                                     DET.MR_REQ_PROC_STEP_DET_NO,
                                     STEP.MR_STEP_CD
                                FROM MR_REQ_MST MR,
				                     MR_REQ_PROC_STEP STEP,
                                     MR_REQ_PROC_STEP_DET DET,
                                     MR_REQ_CHRGR_CHG_HIST HIST
                               WHERE MR.MR_REQ_NO = DET.MR_REQ_NO
				                 AND STEP.MR_REQ_PROC_STEP_NO = DET.MR_REQ_PROC_STEP_NO
                                 AND STEP.MR_STEP_CD = DET.MR_STEP_CD
                                 AND DET.MR_REQ_PROC_STEP_DET_NO = HIST.MR_REQ_PROC_STEP_DET_NO
                                 AND HIST.CHRGR_CL_CD LIKE 'Z02H%'
                                 AND STEP.PROC_ST_CD != 'Z0143'
                                 AND STEP.EFF_END_DTM = '99991231235959'
                                 AND HIST.EFF_END_DTM = '99991231235959'
                                 AND HIST.CHRG_EMP_NO = #{loginEmpNo}
				                 AND MR.DEL_YN=0
                                 AND DET.DEL_YN=0
                                 AND HIST.DEL_YN=0)GRP_STEP
                       WHERE HIST.MR_REQ_PROC_STEP_DET_NO = GRP_STEP.MR_REQ_PROC_STEP_DET_NO
                         AND HIST.CHRG_EMP_NO = #{loginEmpNo}
                      GROUP BY GRP_STEP.MR_REQ_NO, HIST.CHRG_EMP_NO, GRP_STEP.MR_STEP_CD
                      UNION ALL
                      SELECT HIST.CHRG_EMP_NO,
                             MIN(DET.MR_REQ_PROC_STEP_DET_NO) MR_REQ_PROC_STEP_DET_NO,
                             DET.MR_REQ_NO,
                             DET.MR_STEP_CD,
                             NVL(RST.MR_RV_RST_NO,0) MR_RV_RST_NO
                        FROM MR_REQ_MST MR,
	                         MR_REQ_PROC_STEP STEP,
                             MR_REQ_PROC_STEP_DET DET,
                             MR_REQ_CHRGR_CHG_HIST HIST,
                             MR_RV_RST RST
                       WHERE MR.MR_REQ_NO = DET.MR_REQ_NO
		                 AND DET.MR_REQ_NO = STEP.MR_REQ_NO
                         AND DET.MR_REQ_PROC_STEP_DET_NO = HIST.MR_REQ_PROC_STEP_DET_NO
                         --AND DET.MR_STEP_CD IN ('Z00P0', 'Z00R0') /*2025-02-14 ijs 허세욱책임 요청*/
                         AND DET.MR_STEP_CD = 'Z00R0'
                         AND STEP.MR_STEP_CD IN ('Z0020', 'Z0025', 'Z0030', 'Z0040')
                         AND STEP.PROC_ST_CD != 'Z0143'
                         AND STEP.EFF_END_DTM = '99991231235959'
                         AND HIST.EFF_END_DTM = '99991231235959'
                         AND HIST.CHRG_EMP_NO = #{loginEmpNo}
				         AND MR.DEL_YN=0
                         AND DET.DEL_YN=0
                         AND STEP.DEL_YN = 0
                         AND HIST.DEL_YN=0
                         AND RST.MR_REQ_NO(+) = DET.MR_REQ_NO
                         AND RST.MR_REQ_PROC_STEP_DET_NO(+) = DET.MR_REQ_PROC_STEP_DET_NO
                         AND RST.DEL_YN(+)    = 0
                       GROUP BY DET.MR_REQ_NO, HIST.CHRG_EMP_NO, DET.MR_STEP_CD, RST.MR_RV_RST_NO
                       /*2025-02-14 ijs 허세욱책임 요청*/
                      UNION ALL
                      SELECT HIST.CHRG_EMP_NO,
                             MIN(DET.MR_REQ_PROC_STEP_DET_NO) MR_REQ_PROC_STEP_DET_NO,
                             DET.MR_REQ_NO,
                             DET.MR_STEP_CD,
                             NVL(RST.MR_RV_RST_NO,0) MR_RV_RST_NO
                        FROM MR_REQ_PROC_STEP STEP,
                             MR_REQ_PROC_STEP_DET DET,
                             MR_REQ_CHRGR_CHG_HIST HIST,
                             MR_RV_RST RST,
                             MR_REQ_MST MR
                       WHERE STEP.MR_REQ_NO = MR.MR_REQ_NO
                         AND DET.MR_REQ_NO  = STEP.MR_REQ_NO
                         AND DET.MR_REQ_PROC_STEP_DET_NO = HIST.MR_REQ_PROC_STEP_DET_NO
                         AND DET.MR_STEP_CD  = 'Z00P0'
                         AND STEP.MR_STEP_CD IN ('Z0020','Z0025', 'Z0030', 'Z0040')
                         AND STEP.PROC_ST_CD != 'Z0143'
                         AND STEP.EFF_END_DTM = '99991231235959'
                         AND HIST.EFF_END_DTM = '99991231235959'
                         AND HIST.CHRG_EMP_NO =  '221174' /**P*/
                         AND DET.DEL_YN  = 0
                         AND STEP.DEL_YN = 0
                         AND HIST.DEL_YN = 0
                         AND RST.MR_REQ_NO(+) = DET.MR_REQ_NO
                         AND RST.MR_REQ_PROC_STEP_DET_NO(+) = DET.MR_REQ_PROC_STEP_DET_NO
                         AND RST.DEL_YN(+)  = 0
                         AND MR.PORC_ACT_YN = 'Y'
                       GROUP BY DET.MR_REQ_NO, HIST.CHRG_EMP_NO, DET.MR_STEP_CD, MR_RV_RST_NO
                       )GRP_HIST
                 WHERE MR.MR_REQ_NO = DET.MR_REQ_NO
				   AND STEP.MR_REQ_PROC_STEP_NO(+) = DET.MR_REQ_PROC_STEP_NO
                   AND STEP.MR_STEP_CD(+) = DET.MR_STEP_CD
                   AND GRP_HIST.MR_REQ_PROC_STEP_DET_NO = DET.MR_REQ_PROC_STEP_DET_NO
                   AND GRP_HIST.CHRG_EMP_NO = #{loginEmpNo}
				   AND MR.DEL_YN=0
    </select>


    <select id="selectTempSummary" resultMap="summaryMapEx">
    --selectTempSummary
                SELECT SUM(CASE WHEN DET.FST_PROC_TRM_DT >= SYSDATE THEN 1 ELSE 0 END) AS COL01,
                       SUM(CASE WHEN SYSDATE >  DET.FST_PROC_TRM_DT THEN 1 ELSE 0 END) AS COL02
                  FROM MR_REQ_MST MR,
                       MR_REQ_PROC_STEP STEP,
                       MR_REQ_PROC_STEP_DET DET,
                      (SELECT CHRG_EMP_NO,
                              MIN(HIST.MR_REQ_PROC_STEP_DET_NO) AS MR_REQ_PROC_STEP_DET_NO,
                              GRP_STEP.MR_REQ_NO
                        FROM MR_REQ_CHRGR_CHG_HIST HIST,
                             (SELECT STEP.MR_REQ_NO,
                                     MIN(DET.MR_REQ_PROC_STEP_DET_NO) AS MR_REQ_PROC_STEP_DET_NO
                                FROM MR_REQ_MST MR,
	                                 MR_REQ_PROC_STEP STEP,
                                     MR_REQ_PROC_STEP_DET DET,
                                     MR_REQ_CHRGR_CHG_HIST HIST
                               WHERE MR.MR_REQ_NO = DET.MR_REQ_NO
		                         AND STEP.MR_REQ_PROC_STEP_NO = DET.MR_REQ_PROC_STEP_NO
                                 AND STEP.MR_STEP_CD = DET.MR_STEP_CD
                                 AND DET.MR_REQ_PROC_STEP_DET_NO = HIST.MR_REQ_PROC_STEP_DET_NO
                                 AND DET.FST_PROC_TRM_DT IS NOT NULL
                                 AND HIST.CHRGR_CL_CD NOT LIKE 'Z02H%'
                                 AND STEP.EFF_END_DTM = '99991231235959'
                                 AND HIST.EFF_END_DTM = '99991231235959'
                                 AND HIST.CHRG_EMP_NO = #{loginEmpNo}
				                 AND MR.DEL_YN=0
                                 AND DET.DEL_YN=0
                                 AND HIST.DEL_YN=0
                                 GROUP BY STEP.MR_REQ_NO
                              UNION ALL
                              SELECT STEP.MR_REQ_NO,
                                     DET.MR_REQ_PROC_STEP_DET_NO
                                FROM MR_REQ_MST MR,
	                                 MR_REQ_PROC_STEP STEP,
                                     MR_REQ_PROC_STEP_DET DET,
                                     MR_REQ_CHRGR_CHG_HIST HIST
                               WHERE MR.MR_REQ_NO = DET.MR_REQ_NO
		                         AND STEP.MR_REQ_PROC_STEP_NO = DET.MR_REQ_PROC_STEP_NO
                                 AND STEP.MR_STEP_CD = DET.MR_STEP_CD
                                 AND DET.MR_REQ_PROC_STEP_DET_NO = HIST.MR_REQ_PROC_STEP_DET_NO
                                 AND DET.FST_PROC_TRM_DT IS NOT NULL
                                 AND HIST.CHRGR_CL_CD LIKE 'Z02H%'
                                 AND STEP.EFF_END_DTM = '99991231235959'
                                 AND HIST.EFF_END_DTM = '99991231235959'
                                 AND HIST.CHRG_EMP_NO = #{loginEmpNo}
				                 AND MR.DEL_YN=0
                                 AND DET.DEL_YN=0
                                 AND HIST.DEL_YN=0)GRP_STEP
                       WHERE HIST.MR_REQ_PROC_STEP_DET_NO = GRP_STEP.MR_REQ_PROC_STEP_DET_NO
                         AND HIST.CHRG_EMP_NO = #{loginEmpNo}
                      GROUP BY GRP_STEP.MR_REQ_NO, HIST.CHRG_EMP_NO
                       )GRP_HIST
                 WHERE MR.MR_REQ_NO = STEP.MR_REQ_NO
                   AND STEP.MR_REQ_PROC_STEP_NO = DET.MR_REQ_PROC_STEP_NO
                   AND STEP.MR_STEP_CD = DET.MR_STEP_CD
                   AND GRP_HIST.MR_REQ_PROC_STEP_DET_NO = DET.MR_REQ_PROC_STEP_DET_NO
                   AND MR.REQ_CL_CD = '0006'
				   AND MR.DEL_YN=0
    </select>
