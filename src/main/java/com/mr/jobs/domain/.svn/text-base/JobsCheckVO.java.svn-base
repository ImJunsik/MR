public class JobsCheckVO extends Domain {

    private int mrHazopDetNo;
    private int mrReqNo;
    private int mrHazopNo;
    private String itemDiv;
    private String itemCd;
    private String rstVal;
    private String detDiv;

    private int delYn;
    @DateTimeFormat(pattern = "yyyy-MM-dd")
    private Date fstRgstDt;
    private String fstRgstr;

    @DateTimeFormat(pattern = "yyyy-MM-dd")
    private Date lastChgDt;
    private String lastChgr;



    public int getMrHazopDetNo() {
        return mrHazopDetNo;
    }

    public void setMrHazopDetNo(int mrHazopDetNo) {
        this.mrHazopDetNo = mrHazopDetNo;
    }

    public int getMrReqNo() {
        return mrReqNo;
    }

    public void setMrReqNo(int mrReqNo) {
        this.mrReqNo = mrReqNo;
    }

    public int getMrHazopNo() {
        return mrHazopNo;
    }

    public void setMrHazopNo(int mrHazopNo) {
        this.mrHazopNo = mrHazopNo;
    }

    public String getItemDiv() {
        return itemDiv;
    }

    public void setItemDiv(String itemDiv) {
        this.itemDiv = itemDiv;
    }

    public String getItemCd() {
        return itemCd;
    }

    public void setItemCd(String itemCd) {
        this.itemCd = itemCd;
    }

    public String getRstVal() {
        return rstVal;
    }

    public void setRstVal(String rstVal) {
        this.rstVal = rstVal;
    }

    public String getDetDiv() {
        return detDiv;
    }

    public void setDetDiv(String detDiv) {
        this.detDiv = detDiv;
    }

    public int getDelYn() {
        return delYn;
    }

    public void setDelYn(int delYn) {
        this.delYn = delYn;
    }

    public Date getFstRgstDt() {
        return fstRgstDt;
    }

    public void setFstRgstDt(Date fstRgstDt) {
        this.fstRgstDt = fstRgstDt;
    }

    public String getFstRgstr() {
        return fstRgstr;
    }

    public void setFstRgstr(String fstRgstr) {
        this.fstRgstr = fstRgstr;
    }

    public Date getLastChgDt() {
        return lastChgDt;
    }

    public void setLastChgDt(Date lastChgDt) {
        this.lastChgDt = lastChgDt;
    }

    public String getLastChgr() {
        return lastChgr;
    }

    public void setLastChgr(String lastChgr) {
        this.lastChgr = lastChgr;
    }


