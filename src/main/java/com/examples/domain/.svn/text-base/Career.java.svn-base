package com.examples.domain;

import com.base.domain.Domain;

/**
 * 사원의 경력 정보
 * 
 * @author 조용상
 * @version 1.0
 * 
 * <pre>
 * 수정일 | 수정자 | 수정내용
 * ---------------------------------------------------------------------
 * 2014.01.27 조용상 최초 작성
 * </pre>
 */
public class Career extends Domain {
    private int careerNo;
    private int employeeNo;
    private String companyName;
    private String officialPositionCode;
    private String officialPositionName;
    private String retirementDate;
    private String chargeJob;

    /**
     * @return 경력번호 (key)
     */
    public int getCareerNo() {
        return careerNo;
    }

    /**
     * @param careerNo 경력번호
     */
    public void setCareerNo(int careerNo) {
        this.careerNo = careerNo;
    }

    /**
     * @return 사원번호
     */
    public int getEmployeeNo() {
        return employeeNo;
    }

    /**
     * @param employeeNo 사원번호
     */
    public void setEmployeeNo(int employeeNo) {
        this.employeeNo = employeeNo;
    }

    /**
     * @return 회사명
     */
    public String getCompanyName() {
        return companyName;
    }

    /**
     * @param companyName 회사명
     */
    public void setCompanyName(String companyName) {
        this.companyName = companyName;
    }

    /**
     * @return 직책코드
     */
    public String getOfficialPositionCode() {
        return officialPositionCode;
    }

    /**
     * @param officialPositionCode 직책코드
     */
    public void setOfficialPositionCode(String officialPositionCode) {
        this.officialPositionCode = officialPositionCode;
    }

    /**
     * @return 직책명
     */
    public String getOfficialPositionName() {
        return officialPositionName;
    }

    /**
     * @param officialPositionName 직책명
     */
    public void setOfficialPositionName(String officialPositionName) {
        this.officialPositionName = officialPositionName;
    }

    /**
     * @return 퇴사일
     */
    public String getRetirementDate() {
        return retirementDate;
    }

    /**
     * @param retirementDate 퇴사일
     */
    public void setRetirementDate(String retirementDate) {
        this.retirementDate = retirementDate;
    }

    /**
     * @return 담당업무
     */
    public String getChargeJob() {
        return chargeJob;
    }

    /**
     * @param chargeJob 담당업무
     */
    public void setChargeJob(String chargeJob) {
        this.chargeJob = chargeJob;
    }
}